{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Amethyst",
  "colors": {
    "primary": "#868aff",
    "light": "#a0a2eb",
    "dark": "#646ac3"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "development",
              "localization"
            ]
          }
        ]
      },
      {
        "tab": "User Manual",
        "groups": [
          {
            "group": "File Types",
            "pages": [
              "/file_types/amethyst_metadata_file",
              "/file_types/amethyst_node_graph"
            ]
          },
          {
            "group": "Nodes",
            "pages": [
              "/nodes/filter_node",
              "/nodes/master_node",
              "/nodes/input_node",
              "/nodes/output_node"
            ]
          }
        ]
      }
    ],
    
    "global": {
      "anchors": [
        {
          "anchor": "Source",
          "href": "https://github.com/Geoxor/amethyst",
          "icon": "code"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/geoxor",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://discord.gg/geoxor"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Submit issue",
      "href": "https://github.com/Geoxor/Amethyst/issues"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/Geoxor/amethyst",
      "discord": "https://discord.gg/geoxor"
    }
  }
}